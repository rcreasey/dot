#!/usr/bin/env bash

sed -i '' "s/zzzeyez/rcreasey/g" ~/.dot/files/bin/widgets/pecan/scss/style.scss
sassc --style expanded ~/.dot/files/bin/widgets/pecan/scss/style.scss > ~/.dot/files/bin/widgets/pecan/style.css &
ps cax | grep bersicht > /dev/null
if [ $? -eq 0 ]; then
	osascript -e 'tell application "UÌˆbersicht" to refresh'
fi
